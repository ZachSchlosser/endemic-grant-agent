  "search_engines": {
    "primary": "brave_search",
    "fallback": "duckduckgo",
    "grant_specific_queries": [
      "site:grants.gov artificial intelligence consciousness",
      "site:foundationcenter.org AI ethics funding 2025",
      "\"research grant\" consciousness studies 2025 deadline",
      "\"funding opportunity\" artificial intelligence philosophy",
      "grant application AI consciousness interdisciplinary"
    ]
  },
  "grant_aggregators": {
    "GrantSpace": {
      "url": "https://grantspace.org",
      "search_endpoint": "/search",
      "requires_auth": false
    },
    "Foundation Directory": {
      "url": "https://fconline.foundationcenter.org", 
      "requires_auth": true,
      "note": "Requires subscription"
    },
    "Candid": {
      "url": "https://candid.org/find-funding",
      "search_endpoint": "/search",
      "requires_auth": false
    },
    "Grants.gov": {
      "url": "https://grants.gov",
      "search_endpoint": "/web/grants/search-grants.html",
      "api_endpoint": "https://api.grants.gov/",
      "requires_auth": false
    }
  },
  "validation_rules": {
    "required_fields": [
      "organization_name",
      "grant_name", 
      "description",
      "amount"
    ],
    "optional_fields": [
      "deadline",
      "link",
      "alignment_score",
      "notes",
      "application_requirements"
    ],
    "minimum_description_length": 50,
    "maximum_description_length": 2000,
    "red_flag_patterns": [
      "consciousness studies",
      "ai for science and society",
      "information integrity initiative \\d{4}",
      "mathematics and physical sciences - .*",
      "generic research grant",
      "unnamed funding opportunity",
      "tbd grant program",
      "placeholder grant.*"
    ],
    "deadline_validation": {
      "supported_formats": [
        "%Y-%m-%d",
        "%Y-%m-%d %H:%M:%S", 
        "%m/%d/%Y",
        "%B %d, %Y"
      ],
      "warning_days_threshold": 7
    }
  }
}