  "transition_patterns": [
    "\\b(furthermore|moreover|additionally|consequently|nevertheless|nonetheless|therefore|thus|hence)\\b",
    "\\b(in conclusion|to conclude|in summary|to summarize|in essence)\\b",
    "\\b(on the other hand|in contrast|conversely|alternatively)\\b",
    "\\b(for instance|for example|such as|namely|specifically)\\b"
  ],
  "buzzword_clusters": [
    ["innovative", "groundbreaking", "revolutionary", "cutting-edge"],
    ["leverage", "utilize", "optimize", "streamline"],
    ["seamless", "integrated", "comprehensive", "holistic"],
    ["transformative", "paradigm", "disruptive", "game-changing"],
    ["robust", "scalable", "dynamic", "flexible"],
    ["pivotal", "crucial", "mission-critical", "key"],
    ["state-of-the-art", "world-class", "best-in-class", "leading"],
    ["synergy", "collaboration", "partnership", "alignment"]
  ],
  "em_dash_threshold": 2,
  "typography_rules": {
    "em_dash_patterns": {
      "definition_patterns": [
        {
          "pattern": "cognitive widgets[\"']?\\s*[—,]\\s*the unconscious mental frameworks",
          "replacement": "cognitive widgets: the unconscious mental frameworks",
          "description": "Definition with colon"
        },
        {
          "pattern": "OntoEdit AI[\"']?\\s*[—,]\\s*the first tool that",
          "replacement": "OntoEdit AI. The first tool that",
          "description": "Dramatic pause with period"
        },
        {
          "pattern": "metaphysical malware[\"']?\\s*[—,]\\s*hidden cognitive",
          "replacement": "metaphysical malware: hidden cognitive",
          "description": "Definition with colon"
        },
        {
          "pattern": "truth-seeking philosophy[\"']?\\s*[—,]\\s*an AI system",
          "replacement": "truth-seeking philosophy. An AI system",
          "description": "Dramatic pause with period"
        }
      ],
      "hyphenation_fixes": [
        {"from": "philosophy-to, code mission", "to": "philosophy-to-code mission"},
        {"from": "open, source", "to": "open-source"},
        {"from": "90, day Phase", "to": "90-day Phase"},
        {"from": "proof-of, concept", "to": "proof-of-concept"},
        {"from": "1, line on how", "to": "1-line on how"},
        {"from": "co — founder", "to": "co-founder"},
        {"from": "subject — object", "to": "subject-object"}
      ],
      "replacement_strategies": {
        "sentence_break": {
          "condition": "right_part_starts_uppercase",
          "replacement": ". ",
          "description": "Period for sentence break"
        },
        "connection": {
          "condition": "contains_connector_words",
          "connector_words": ["and", "but", "which", "that"],
          "replacement": ", ",
          "description": "Comma for connection"
        },
        "default": {
          "replacement": ", ",
          "description": "Default comma"
        }
      },
      "target_max_per_document": 6,
      "max_per_section": 2
    },
    "dash_characters": {
      "em_dash": "—",
      "en_dash": "–", 
      "hyphen": "-",
      "minus": "−",
      "figure_dash": "‒",
      "quotation_dash": "―"
    }
  },
  "formal_indicators": [
    "furthermore", "moreover", "consequently", "nevertheless", "therefore",
    "henceforth", "notwithstanding", "accordingly", "subsequently", "wherein"
  ],
  "casual_replacements": {
    "furthermore": ["Also", "Plus", "And"],
    "moreover": ["Plus", "Also", "What's more"],
    "consequently": ["So", "This means", "As a result"],
    "nevertheless": ["But", "Still", "Even so"],
    "therefore": ["So", "This means", "That's why"],
    "henceforth": ["From now on", "Going forward", "Next"],
    "notwithstanding": ["Despite", "Even with", "Still"],
    "accordingly": ["So", "Therefore", "This way"],
    "subsequently": ["Then", "Next", "After that"],
    "wherein": ["where", "in which", "that"]
  },
